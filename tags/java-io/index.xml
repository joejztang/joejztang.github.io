&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;&lt;span style="color:#6e7681"&gt;				&lt;/span&gt;fr.close();&lt;span style="color:#6e7681"&gt;
